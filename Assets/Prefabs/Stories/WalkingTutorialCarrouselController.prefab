      objectReference: {fileID: 139754, guid: 37391a100bd51f947ae06b8ce937e2c5, type: 2}
    - target: {fileID: 0}
      propertyPath: items.Array.data[20].text
      value: "Para llenar esta barra, usa los siguientes objetos:  Algunos objetos
        no suben la barra, pero quitan problemas de salud que s\xED la bajan."
      objectReference: {fileID: 0}
    - target: {fileID: 0}
      propertyPath: items.Array.data[21].text
