      objectReference: {fileID: 0}
    - target: {fileID: 0}
      propertyPath: items.Array.data[8].text
      value: Para abrir tu mochila, toca la mochila de abajo
      objectReference: {fileID: 0}
    - target: {fileID: 0}
      propertyPath: items.Array.data[9].text
      value: Ahora puedes usar las cosas que empacaste.
      objectReference: {fileID: 0}
    - target: {fileID: 0}
